server {
    listen 80;
    server_name _;

    location / {
        try_files $uri $uri/ =404;
    }

    location /airbnb-onepage/ {
        proxy_pass http://localhost:5000;
        include proxy_params;
    }

    location ~ ^/airbnb-dynamic/number_odd_or_even/(.*)$ {
        proxy_pass http://localhost:5001/number_odd_or_even/$1;
        include proxy_params;
    }
}
